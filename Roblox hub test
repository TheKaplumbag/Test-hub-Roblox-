local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Test hub", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

--[[
Name = "Test UI"
HidePremium = false
SaveConfig = false
ConfigFolder = nil
IntroEnabled = true
IntroText = "This is intro"
IntroIcon = nil
Icon = nil
CloseCallback = <function> - Function to execute when the window is closed.
]]
local Tab = Window:MakeTab({
	Name = "Tab 1",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

--[[
Name = "Test tab"
Icon = https://media.discordapp.net/attachments/1138377138475642932/1288968232350257265/HubThumbnail.png?ex=66f71cb6&is=66f5cb36&hm=d79af648dcd7704373b2111a3026095784bc3a04b4246294f2e0c1b47b0e7e00&
PremiumOnly = false
]]
local Section = Tab:AddSection({
	Name = "Section"
})

--[[
Name = "Test section"
]]

Tab:AddButton({
	Name = "Button",
	Callback = function()
      		print("Printed")
  	end    
})

--[[
Name = "Button"
Callback = <function> - The function of the button.
]]

OrionLib:Init()
