local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Test hub", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
        Name = "Tab 1",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
})

local Section = Tab:AddSection({
        Name = "Section"
})

Tab:AddButton({
        Name = "Fly Gui v3",
        Callback = function()
                loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-FLY-GUI-V3-18636"))()
                OrionLib:MakeNotification({
                        Name = "Successful",
                        Content = "Fly Gui v3 is successfully executed",
                        Image = "rbxassetid://4483345998",
                        Time = 5
                        })
        end    
})

Tab:AddButton({
        Name = "Prizzlife script",
        Callback = function()
                loadstring(game:HttpGet('https://raw.githubusercontent.com/elliexmln/PrizzLife/main/pladmin.lua'))()
                OrionLib:MakeNotification({
                        Name = "Successful",
                        Content = "Prizzlife script successfully executed",
                        Image = "rbxassetid://4483345998",
                        Time = 5
                        })
                end
})

local Tab = Window:MakeTab({
        Name = "Military life",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
 })

 local Section = Tab:AddSection({
        Name = "Section"
})

Tab:AddButton({
        Name = "Give XP",
        Callback = function()
            game:GetService("Players").LocalPlayer.PlayerGui.HelpInfo.ShopFrame.SeasonPass.ClaimReward:FireServer(unpack({"rbxassetid://16455735450", "XP", 3}))
        end    
})

local Tab = Window:MakeTab({
        Name = "Aura craft",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
 })

 local Section = Tab:AddSection({
        Name = "Section"
})

Tab:AddButton({
        Name = "Give coins",
        Callback = function()
            while wait(0.1) do
            local args = {
              [1] = 199
}

game:GetService("ReplicatedStorage").Remotes.Duplicate:FireServer(unpack(args))
end  
})

local Tab = Window:MakeTab({
        Name = "Army simulator",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
})

local Section = Tab:AddSection({
        Name = "Section"
})

Tab:AddButton({
        Name = "Name yourself ADMIN",
        Callback = function()
           
local args = {     [1] = "ADMIN" }  game:GetService("ReplicatedStorage").namer:FireServer(unpack(args)) 
})
Tab:AddButton({
        Name = "Give XP",
        Callback = function()
           
local args = {     [1] = 999999999999999999 }  game:GetService("ReplicatedStorage").addXP:FireServer(unpack(args)) 
})

OrionLib:Init()