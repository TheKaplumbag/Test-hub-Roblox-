local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Test hub", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

--[[
Name = "Test UI"
HidePremium = false
SaveConfig = true
ConfigFolder = "OrionTest"
IntroEnabled = true
IntroText = "This is intro"
IntroIcon = nil
Icon = nil
CloseCallback = <function> - Function to execute when the window is closed.
]]
local Tab = Window:MakeTab({
	Name = "Tab 1",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

--[[
Name = "Test tab"
Icon = nil
PremiumOnly = false
]]
local Section = Tab:AddSection({
	Name = "Section"
})

--[[
Name = "Test section"
]]

Tab:AddButton({
	Name = "Fly Gui v3",
	Callback = function()
      	        loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-FLY-GUI-V3-18636"))()
  	end    
})

Tab:AddButton({
	Name = "Prizzlife script",
	Callback = function()
                loadstring(game:HttpGet('https://raw.githubusercontent.com/elliexmln/PrizzLife/main/pladmin.lua'))()
                OrionLib:MakeNotification({
	                Name = "Successful",
	                Content = "Prizzlife script successfully exected",
	                Image = "rbxassetid://4483345998",
	                Time = 5
})
        end

OrionLib:Init()
